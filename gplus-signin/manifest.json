{
  "name": "Google+ Sign-In & Chrome Demo",
  "version": "1",
  "manifest_version": 2,
  "minimum_chrome_version": "23",
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions": ["experimental"],
  "oauth2": {
    "client_id": "{YOUR_INSTALLED_CLIENT}",
    "scopes": ["https://www.googleapis.com/auth/plus.login"]
  }
}
